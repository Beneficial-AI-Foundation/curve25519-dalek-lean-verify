{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Dalek Verify Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-aeneas-verify"
      ],
      "outFiles": [
        "${workspaceFolder}/vscode-aeneas-verify/out/**/*.js"
      ],
      "preLaunchTask": "npm: compile - vscode-aeneas-verify"
    }
  ]
}
